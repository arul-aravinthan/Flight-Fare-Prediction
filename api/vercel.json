{
  "version": 2,
  "builds": [
      {
          "src": "./app.py",
          "use": "@vercel/python"
      }
  ],
  "rewrites": [
      { "source": "/(.*)", "destination": "src/app.py" }
  ],
  "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      },
      {
        "source": "https://flight-fare-prediction-react-frontend.vercel.app/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }  
  ]
}